<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="504.0" prefWidth="586.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="builder.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Pane fx:id="pane" prefHeight="500.0" prefWidth="546.0" />

    <HBox prefHeight="100.0" prefWidth="200.0">
        <children>

            <Label prefHeight="17.0" prefWidth="102.0" text="Введите время: ">
                <HBox.margin>
                    <Insets top="3.0" />
                </HBox.margin>
            </Label>

            <TextField fx:id="textField">
                <HBox.margin>
                    <Insets left="20.0" />
                </HBox.margin>
            </TextField>

            <Button mnemonicParsing="false" onAction="#indicator" prefHeight="25.0" prefWidth="81.0" text="Go">
                <HBox.margin>
                    <Insets left="50.0" />
                </HBox.margin>
            </Button>

        </children>
    </HBox>
</VBox>
